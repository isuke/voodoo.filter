Hide "Flask"
    Class "Utility Flask" "Life Flask" "Mana Flask" "Hybrid Flask"
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Flask Color")
    SetBackgroundColor Var("Cheap BG Color") 60
    SetBorderColor Var("Flask Color")
    CustomAlertSound "./Sounds/Flask.wav"

    Mixin "Class"
        Prop("Utility Flask Activity") "Utility"
            Class "Utility Flask"
            SetFontSize Var("Default Font Size")
            SetTextColor Var("Flask Color")
            SetBackgroundColor Var("Precious BG Color")
            SetBorderColor Var("Precious Color")
            # MinimapIcon NONE
            # PlayEffect NONE
            # PlayAlertSoundPositional NONE
        Show "Life/Mana/Hybrid"
            Class "Life Flask" "Mana Flask" "Hybrid Flask"

            Mixin "Size"
                Prop("Flask 1 Activity") "Small/Medium"
                    DropLevel >= 0
                    SetBackgroundColor Var("Cheap BG Color") 60
                Prop("Flask 2 Activity") "Large/Greater/Grand/Giant/Colossal"
                    DropLevel >= 3
                    SetBackgroundColor Var("Trivial BG Color")
                Prop("Flask 3 Activity") "Sacred/Hallowed/Sanctified"
                    DropLevel >= 30
                    SetBackgroundColor Var("Precious BG Color")
                Prop("Flask 4 Activity") "Divine/Eternal"
                    DropLevel >= 60
                    SetBackgroundColor Var("Lofty BG Color")

    Mixin "Quality"
        Prop("Quality Flask Activity") "High"
            Quality > 10
            SetFontSize Var("Default Font Size")
            SetTextColor Var("Flask Color")
            SetBackgroundColor Var("Tier2 BG Color")
            SetBorderColor Var("Trivial Color")
            # MinimapIcon NONE
            PlayEffect Var("Trivial PlayEffect Color") Temp
            PlayAlertSoundPositional Var("Trivial Sound Id")
        Prop("Quality Flask Activity") "Middle"
            Quality > 0
            SetFontSize Var("Default Font Size")
            SetTextColor Var("Flask Color")
            SetBackgroundColor Var("Tier1 BG Color")
            SetBorderColor Var("Trivial Color")
            # MinimapIcon NONE
            PlayEffect Var("Trivial PlayEffect Color") Temp
            PlayAlertSoundPositional Var("Trivial Sound Id")

    Mixin "Over Quality"
        Show "True"
            Quality > 20
            SetFontSize Var("Extra Large Font Size")
            SetTextColor 255 255 255
            SetBackgroundColor Var("Flask Color")
            SetBorderColor Var("Sublime Color")
            MinimapIcon Largest Var("Sublime Minimap Icon Color") Var("Flask Minimap Icon Shape")
            PlayEffect Var("Sublime PlayEffect Color")
            PlayAlertSoundPositional Var("Sublime Sound Id")

