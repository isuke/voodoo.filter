# DONE
Hide "Currency"
    Class "Currency"
    BaseType Var("Currencies")
    SetTextColor Var("Currency Color")

    Mixin "Rareness"
        Show "Sublime"
            BaseType Var("Sublime Currencies")
            SetFontSize Var("Extra Large Font Size")
            SetTextColor 0 0 0
            SetBackgroundColor Var("Currency Color")
            SetBorderColor Var("Sublime Color")
            MinimapIcon Largest Var("Sublime Minimap Icon Color") Var("Currency Item Minimap Icon Shape")
            PlayEffect Var("Sublime PlayEffect Color")
            PlayAlertSoundPositional Var("Sublime Sound Id")

            Mixin "BaseType"
                Show "Mirror of Kalandra"
                    BaseType "Mirror of Kalandra"
                    PlayAlertSound ShMirror 300
                Show "Divine Orb"
                    BaseType "Divine Orb"
                    PlayAlertSound ShDivine 300
        Show "Lofty"
            BaseType Var("Lofty Currencies")
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Lofty BG Color")
            SetBorderColor Var("Lofty Color")
            MinimapIcon Medium Var("Lofty Minimap Icon Color") Var("Currency Item Minimap Icon Shape")
            PlayEffect Var("Lofty PlayEffect Color")
            PlayAlertSoundPositional Var("Lofty Sound Id")

            Mixin "BaseType"
                Show "Chaos Orb"
                    BaseType "Chaos Orb"
                    PlayAlertSound ShChaos 300
        Show "Precious"
            BaseType Var("Precious Currencies")
            SetFontSize Var("Default Font Size")
            SetBackgroundColor Var("Precious BG Color")
            SetBorderColor Var("Precious Color")
            MinimapIcon Small Var("Precious Minimap Icon Color") Var("Currency Item Minimap Icon Shape")
            PlayEffect Var("Precious PlayEffect Color")
            PlayAlertSoundPositional Var("Precious Sound Id")
        Prop("Trivial Currency Activity") "Trivial"
            BaseType Var("Trivial Currencies")
            SetFontSize Var("Default Font Size")
            SetBackgroundColor Var("Trivial BG Color")
            SetBorderColor Var("Trivial Color")
            # MinimapIcon NONE
            PlayEffect Var("Trivial PlayEffect Color") Temp
            PlayAlertSoundPositional Var("Trivial Sound Id")
        Prop("Cheap Currency Activity") "Cheap"
            BaseType Var("Cheap Currencies")
            SetFontSize Var("Small Font Size")
            SetBackgroundColor Var("Cheap BG Color") 60
            SetBorderColor Var("Cheap Color")
            # MinimapIcon NONE
            # PlayEffect NONE
            PlayAlertSoundPositional Var("Cheap Sound Id")

            Mixin "Stack Size"
                Show "High Stack Size"
                    StackSize >= 15
                    SetBackgroundColor Var("Cheap BG Color")
                    MinimapIcon Small Var("Precious Minimap Icon Color") Var("Currency Item Minimap Icon Shape")
                    PlayEffect Var("Trivial PlayEffect Color") Temp
                    # PlayAlertSound 3 300

