Prop("Chromatic Orb Recipe Activity") "Chromatic Orb Recipe"
    Class Var("Weapons") Var("Gears")
    SocketGroup RGB
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Chromatic Orb Recipe Color")
    SetBackgroundColor 0 0 0 200
    SetBorderColor Var("Chromatic Orb Recipe Color")
    MinimapIcon Small Var("Trivial Effect Color") Var("Equipment Minimap Icon Shape")
    PlayEffect Var("Trivial Effect Color") Temp

    Fork "Size"
        Unset "Small1"
            Height <= 3
            Width <= 1
        Unset "Small2"
            Height <= 2
            Width <= 2

Hide "Other Equipments"
    Class Var("Equipments")
    SetFontSize Var("Small Font Size")
    SetBackgroundColor 0 0 0 180

################################################################################
# Currencies                                                                   #
################################################################################
Hide "Currency"
    Class "Currency"
    BaseType == Var("Currencies")
    SetTextColor Var("Currency Color")

    Fork "Rareness"
        Show "Sublime"
            BaseType == Var("Sublime Currencies")
            SetFontSize Var("Extra Large Font Size")
            SetBackgroundColor Var("Sublime BG Color")
            MinimapIcon Largest Var("Sublime Effect Color") Var("Currency Minimap Icon Shape")
            PlayEffect Var("Sublime Effect Color")
            PlayAlertSoundPositional Var("Sublime Sound")

            Mixin "Base Type"
                Unset "Mirror of Kalandra"
                    BaseType == "Mirror of Kalandra"
                    # PlayAlertSound ShMirror 300
                    CustomAlertSound "./Sounds/Mirror.wav"
                    DisableDropSound
                Unset "Exalted Orb"
                    BaseType == "Exalted Orb"
                    CustomAlertSound "./Sounds/Exalted.wav"
                    DisableDropSound
                Unset "Divine Orb"
                    BaseType == "Divine Orb"
                    # PlayAlertSound ShDivine 300
                    CustomAlertSound "./Sounds/Divine.wav"
                    DisableDropSound
        Show "Lofty"
            BaseType == Var("Lofty Currencies")
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Lofty BG Color")
            MinimapIcon Medium Var("Lofty Effect Color") Var("Currency Minimap Icon Shape")
            PlayEffect Var("Lofty Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")

            Mixin "Base Type"
                Unset "Chaos Orb"
                    BaseType == "Chaos Orb"
                    # PlayAlertSound ShChaos 300
                    CustomAlertSound "./Sounds/Chaos.wav"
                    DisableDropSound
        Show "Precious"
            BaseType == Var("Precious Currencies")
            SetFontSize Var("Default Font Size")
            SetBackgroundColor Var("Precious BG Color")
            MinimapIcon Small Var("Precious Effect Color") Var("Currency Minimap Icon Shape")
            PlayEffect Var("Precious Effect Color")
            PlayAlertSoundPositional Var("Precious Sound")

        Prop("Trivial Currency Activity") "Trivial"
            BaseType == Var("Trivial Currencies")
            SetFontSize Var("Default Font Size")
            SetBackgroundColor Var("Trivial BG Color") 240
            MinimapIcon Small Var("Trivial Effect Color") Var("Currency Minimap Icon Shape")
            PlayEffect Var("Trivial Effect Color") Temp
            # PlayAlertSoundPositional Var("Trivial Sound")

            Mixin "Stack Size"
                Show "High Stack Size"
                    StackSize >= 15
        Prop("Cheap Currency Activity") "Cheap"
            BaseType == Var("Cheap Currencies")
            SetFontSize Var("Default Font Size")
            SetBackgroundColor Var("Cheap BG Color") 200
            # MinimapIcon NONE
            # PlayEffect NONE
            # PlayAlertSoundPositional Var("Cheap Sound")

            Mixin "Stack Size"
                Show "High Stack Size"
                    StackSize >= 15
