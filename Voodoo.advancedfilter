################################################################################
# Equipments                                                                   #
################################################################################
Show "Fishing Rod"
    Class "Fishing Rods"
    SetFontSize Var("Extra Large Font Size")
    SetTextColor Var("Weapon Color")
    SetBackgroundColor Var("Sublime BG Color")
    # SetBorderColor NONE
    MinimapIcon Large Var("Sublime Effect Color") Var("Equipment Minimap Icon Shape")
    PlayEffect Var("Sublime Effect Color")
    PlayAlertSoundPositional Var("Sublime Sound")

    Mixin "Rarity"
        Show "Unique"
            Rarity Unique
            SetTextColor Var("Unique Item Color")

#
# Jewelry(Amulet/Ring)
#
Show "Unique Jewelry"
    Class Var("Jewelries")
    Rarity Unique
    SetFontSize Var("Large Font Size")
    SetTextColor Var("Unique Item Color")
    MinimapIcon Large Var("Unique Effect Color") Var("Accessory Minimap Icon Shape")
    PlayEffect Var("Unique Effect Color")
    PlayAlertSoundPositional Var("Unique Sound")
    CustomAlertSoundOptional "./Sounds/Unique.wav" 300

    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

Show "Special Base Type Jewelry"
    Class Var("Jewelries")

    Fork "Base Type"
        Show "Experimented"
            BaseType Var("Experimented Base Jewelries")
            SetFontSize Var("Extra Large Font Size")
            SetBackgroundColor Var("Experimented Color")
            MinimapIcon Large Var("Experimented Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Experimented Effect Color")
            PlayAlertSoundPositional Var("Sublime Sound")

            Mixin "Item Level"
                Show "High"
                    ItemLevel >= 85
                    SetBorderColor Var("High Item Level Border Color")

        Show "Atlas"
            BaseType Var("Atlas Base Jewelries")
            SetFontSize Var("Default Font Size")
            SetBackgroundColor Var("Atlas Color")
            PlayEffect Var("Atlas Effect Color") Temp
            PlayAlertSoundPositional Var("Precious Sound")

            Mixin "Item Level"
                Show "High"
                    ItemLevel >= 85
                    MinimapIcon Small Var("Atlas Effect Color") Var("Accessory Minimap Icon Shape")
                    SetBorderColor Var("High Item Level Border Color")

    Fork "Rarity"
        Unset "Rare"
            Rarity Rare
            SetTextColor Var("Rare Item Color")
        Unset "Magic"
            Rarity Magic
            SetTextColor Var("Magic Item Color")
        Unset "Normal"
            Rarity Normal
            SetTextColor Var("Normal Item Color")

    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

Hide "Talisman"
    BaseType "Talisman"
    SetFontSize Var("Extra Small Font Size")
    SetTextColor Var("Rare Item Color")
    SetBackgroundColor Var("Talisman Color") 160

Hide "Breach Ring"
    BaseType "Breach Ring"
    SetFontSize Var("Extra Small Font Size")
    SetTextColor Var("Rare Item Color")
    SetBackgroundColor Var("Breach Color") 160

Prop("Rare Accessory Activity") "Rare Jewelry"
    Class Var("Jewelries")
    Rarity Rare
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Rare Item Color")
    SetBackgroundColor Var("Jewelry Color") 200

    Mixin "Base Type"
        Unset "Good"
            BaseType Var("Good Jewelries")
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Jewelry Color") 255

    Mixin "Special"
        Show "Influence"
            HasInfluence "Shaper" "Elder" "Crusader" "Hunter" "Redeemer" "Warlord"
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Influence Color")
            MinimapIcon Medium Var("Influence Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Influence Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")
        Unset "Fractured"
            FracturedItem True
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Synthesis Color") 200
            PlayAlertSoundPositional Var("Lofty Sound")
        Unset "Synthesised"
            SynthesisedItem True
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Synthesis Color")
            PlayAlertSoundPositional Var("Lofty Sound")

    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

    Fork "Identified"
        Unset "False"
            Identified False
        Unset "True"
            Identified True

            Mixin "Mods"
                Show "Bench Craft"
                    HasExplicitMod Var("Bench Craft Mods")
                    SetBackgroundColor Var("Crafted Mod Color")
                    # MinimapIcon Medium Var("Bench Craft Effect Color") Var("Accessory Minimap Icon Shape")
                    # PlayEffect Var("Bench Craft Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Incursion"
                    HasExplicitMod Var("Incursion Mods")
                    SetBackgroundColor Var("Incursion Color")
                    # MinimapIcon Medium Var("Incursion Effect Color") Var("Accessory Minimap Icon Shape")
                    # PlayEffect Var("Incursion Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Delve"
                    HasExplicitMod Var("Delve Mods")
                    SetBackgroundColor Var("Delve Color")
                    # MinimapIcon Medium Var("Delve Effect Color") Var("Accessory Minimap Icon Shape")
                    # PlayEffect Var("Delve Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Betrayal1"
                    HasExplicitMod Var("Betrayal General Mods")
                    SetBackgroundColor Var("Betrayal Color")
                    # MinimapIcon Medium Var("Betrayal Effect Color") Var("Accessory Minimap Icon Shape")
                    # PlayEffect Var("Betrayal Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Betrayal2"
                    HasExplicitMod Var("Betrayal Member's Mods")
                    SetBorderColor Var("Betrayal Member's Mod Color")
                    SetBackgroundColor Var("Betrayal Color")
                    # MinimapIcon Medium Var("Betrayal Effect Color") Var("Accessory Minimap Icon Shape")
                    PlayEffect Var("Betrayal Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")

Unset "Normal/Magic Jewelry"
    Class Var("Jewelries")
    SetFontSize Var("Small Font Size")
    SetBackgroundColor Var("Jewelry Color") 200

    Mixin "Base Type"
        Unset "Good"
            BaseType Var("Good Jewelries")
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Jewelry Color") 255

    Fork "Rarity"
        Prop("Magic Equipment Activity") "Magic"
            Rarity Magic
            SetTextColor Var("Magic Item Color")
        Unset "Normal"
            Rarity Normal
            SetTextColor Var("Normal Item Color")
    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

