# Voodoo Filter

Unset "Equipments"
    Class Var("Equipments")
    SetFontSize Var("Small Font Size")

    Fork "Class 0"
        Unset "Accessory"
            Class Var("Accessories")

            Fork "Class 1"
                Unset "Jewelry"
                    Class Var("Jewelries")
                    SetBackgroundColor Var("Jewelry Color") 200

                    Mixin "Base Type"
                        Show "Atlas Base Accessories"
                            BaseType Var("Atlas Base Accessories")
                            SetFontSize Var("Extra Large Font Size")
                            SetBackgroundColor Var("Atlas Color")
                        Show "Talisman"
                            BaseType "Talisman"
                            SetBackgroundColor Var("Talisman Color")
                        Hide "Breach Ring"
                            BaseType "Breach Ring"
                            SetBackgroundColor Var("Breach Color") 200
                        Unset "Good"
                            BaseType Var("Good Amulets") Var("Good Rings")
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Lighten(50%)

                            Mixin "Item Level"
                                Show "High"
                                    ItemLevel >= Prop("High Item Level")
                                    SetBorderColor Var("High Item Level Color")

                Unset "Belt"
                    Class "Belt"
                    SetBackgroundColor Var("Belt Color") 200

                    Mixin "Base Type"
                        Show "Atlas Base Accessories"
                            BaseType Var("Atlas Base Accessories")
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Var("Atlas Color")
                        Show "Abyss"
                            BaseType "Stygian Vise"
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Var("Abyss Color")
                        Unset "Good"
                            BaseType Var("Good Belts")
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Lighten(50%)

                            Mixin "Item Level"
                                Show "High"
                                    ItemLevel >= Prop("High Item Level")
                                    SetBorderColor Var("High Item Level Color")

        Unset "Weapon and Gear"
            Class Var("Weapons") Var("Gears")

            Fork "Class 1"
                Unset "Weapon"
                    Class Var("Weapons")
                    SetBackgroundColor Var("Weapon Color") 200

                    Mixin "Base Type"
                        Unset "Good"
                            BaseType Var("Good Weapons")
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Lighten(50%)

                            Mixin "Item Level"
                                Show "High"
                                    ItemLevel >= Prop("High Item Level")
                                    SetBorderColor Var("High Item Level Color")

                Unset "Gear"
                    Class Var("Gears")
                    SetBackgroundColor Var("Gear Color") 200

                    Mixin "Base Type"
                        Show "Atlas Base Gears"
                            BaseType Var("Atlas Base Gears") Var("Atziri Gears")
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Var("Atlas Color") 200
                        Unset "Good"
                            BaseType Var("Good Gears")
                            SetFontSize Var("Large Font Size")
                            SetBackgroundColor Lighten(50%)

                            Mixin "Item Level"
                                Show "High"
                                    ItemLevel >= Prop("High Item Level")
                                    SetBorderColor Var("High Item Level Color")

            Mixin "Socket"
                Show "6L"
                    LinkedSockets = 6
                    SetFontSize Var("Extra Large Font Size")
                    SetTextColor Var("Weapon Color")
                    SetBackgroundColor Var("Sublime BG Color")
                    MinimapIcon Largest Var("Sublime Minimap Icon Color") Var("Equipment Minimap Icon Shape")
                    PlayEffect Var("Sublime PlayEffect Color")
                    PlayAlertSoundPositional Var("Sublime Sound")
                Show "6S"
                    Sockets = 6
                    SetFontSize Var("Large Font Size")
                    SetTextColor Var("Weapon Color")
                    SetBackgroundColor Var("Lofty BG Color")
                    MinimapIcon Medium Var("Lofty Minimap Icon Color") Var("Equipment Minimap Icon Shape")
                    PlayEffect Var("Lofty PlayEffect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Prop("5L Activity") "5L"
                    LinkedSockets = 5
                    SetFontSize Var("Default Font Size")
                    SetTextColor Var("Weapon Color")
                    SetBackgroundColor Var("Precious BG Color")
                    MinimapIcon Small Var("Precious Minimap Icon Color") Var("Equipment Minimap Icon Shape")
                    PlayEffect Var("Precious PlayEffect Color")
                    PlayAlertSoundPositional Var("Precious Sound")
    Fork "Rarity"
        Show "Unique"
            Rarity Unique
            SetTextColor Var("Unique Item Color")
        Unset "Rare"
            Rarity Rare
            SetTextColor Var("Rare Item Color")

            Mixin "Rare BaseType"
                Prop("Rare Jewelry Activity") "Jewelry"
                    Class Var("Jewelries")
                Prop("Rare Belt Activity") "Belt"
                    Class "Belt"
                Prop("Rare Weapon and Gear Activity") "Weapon and Gear"
                    Class Var("Weapons") Var("Gears")

            Mixin "Shaper/Elder"
                Show "Shaper"
                    ShaperItem True
                    SetBackgroundColor Var("Shaper/Elder Color")
                Show "Elder"
                    ElderItem True
                    SetBackgroundColor Var("Shaper/Elder Color")
        Unset "Magic"
            Rarity Magic
            SetTextColor Var("Magic Item Color")
        Unset "Normal"
            Rarity Normal
            SetTextColor Var("Normal Item Color")
    Mixin "Corrupted"
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")
    Mixin "Identified"
        Unset "True"
            Identified True

            Mixin "Mods"
                Show "Benche Craft"
                    HasExplicitMod Var("Bench Craft Mods")
                    SetBackgroundColor Var("Crafted Mod Color")
                Show "Incursion"
                    HasExplicitMod Var("Incursion Mods")
                    SetBackgroundColor Var("Incursion Color")
                Show "Delve"
                    HasExplicitMod Var("Delve Mods")
                    SetBackgroundColor Var("Delve Color")
