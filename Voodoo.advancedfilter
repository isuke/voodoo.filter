Show "Unique Weapon/Gear"
    Class Var("Weapons") Var("Gears")
    Rarity Unique
    SetFontSize Var("Large Font Size")
    SetTextColor Var("Unique Item Color")
    CustomAlertSound "./Sounds/Unique.wav"
    DisableDropSound True

    Mixin "Drop Level"
        Show "High"
            DropLevel >= 50
            MinimapIcon Largest Var("Unique Effect Color") Var("Equipment Minimap Icon Shape")
            PlayEffect Var("Unique Effect Color")

    Mixin "Socket"
        Show "6L"
            LinkedSockets = 6
            SetFontSize Var("Extra Large Font Size")
            SetTextColor Var("Weapon Color")
            SetBackgroundColor Var("Sublime BG Color")
            SetBorderColor Var("6L Color")
            MinimapIcon Largest Var("Sublime Effect Color") Var("Equipment Minimap Icon Shape")
            PlayEffect Var("Sublime Effect Color")
            PlayAlertSoundPositional Var("Sublime Sound")
        Prop("5L Activity") "5L"
            LinkedSockets = 5
            SetBackgroundColor Var("Precious BG Color")
            SetBorderColor Var("3L/4L/5L Color")
            MinimapIcon Small Var("Precious Effect Color") Var("Equipment Minimap Icon Shape")
            PlayEffect Var("Precious Effect Color")
            PlayAlertSoundPositional Var("Precious Sound")
        Prop("4L Activity") "4L"
            LinkedSockets = 4
            SetBackgroundColor Var("Trivial BG Color")
            SetBorderColor Var("3L/4L/5L Color")
            # MinimapIcon NONE
            PlayEffect Var("Trivial Effect Color") Temp
            PlayAlertSoundPositional Var("Cheap Sound")
        Prop("3L Activity") "3L"
            LinkedSockets = 3
            SetBackgroundColor Var("Trivial BG Color")
            SetBorderColor Var("3L/4L/5L Color")
            # MinimapIcon NONE
            PlayEffect Var("Trivial Effect Color") Temp
            PlayAlertSoundPositional Var("Cheap Sound")

    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

Prop("Rare Weapon/Gear Activity") "Rare Weapon/Gear"
    Class Var("Weapons") Var("Gears")
    Rarity Rare
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Rare Item Color")

    Fork "Class"
        Unset "Weapon"
            Class Var("Weapons")
            SetBackgroundColor Var("Weapon Color") 200

            Mixin "Base Type"
                Show "Atlas Base Weapons"
                    BaseType Var("Atlas Base Weapons")
                    SetFontSize Var("Large Font Size")
                    SetBackgroundColor Var("Atlas Color")
                    # MinimapIcon Small Var("Atlas Effect Color") Var("Equipment Minimap Icon Shape")
