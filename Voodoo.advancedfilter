# Voodoo Filter

Show "Fishing Rod"
    Class "Fishing Rods"
    SetFontSize Var("Extra Large Font Size")
    SetTextColor Var("Weapon Color")
    SetBackgroundColor Var("Sublime BG Color")
    # SetBorderColor NONE
    MinimapIcon Largest Var("Sublime Effect Color") Var("Equipment Minimap Icon Shape")
    PlayEffect Var("Sublime Effect Color")
    PlayAlertSoundPositional Var("Sublime Sound")

    Mixin "Rarity"
        Show "Unique"
            Rarity Unique
            SetTextColor Var("Unique Item Color")

Show "Unique Jewelry"
    Class Var("Jewelries")
    Rarity Unique
    SetFontSize Var("Large Font Size")
    SetTextColor Var("Unique Item Color")
    MinimapIcon Largest Var("Unique Effect Color") Var("Accessory Minimap Icon Shape")
    PlayEffect Var("Unique Effect Color")
    CustomAlertSound "./Sounds/Unique.wav"
    DisableDropSound True

    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

Prop("Rare Jewelry Activity") "Rare Jewelry"
    Class Var("Jewelries")
    Rarity Rare
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Rare Item Color")
    SetBackgroundColor Var("Jewelry Color") 200

    Mixin "Base Type"
        Show "Atlas Base Accessories"
            BaseType Var("Atlas Base Accessories")
            SetFontSize Var("Extra Large Font Size")
            SetBackgroundColor Var("Atlas Color")
            MinimapIcon Medium Var("Atlas Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Atlas Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")
        Show "Talisman"
            BaseType "Talisman"
            SetBackgroundColor Var("Talisman Color")
            MinimapIcon Medium Var("Talisman Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Talisman Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")
        Hide "Breach Ring"
            BaseType "Breach Ring"
            SetFontSize Var("Small Font Size")
            SetBackgroundColor Var("Breach Color") 160
        Unset "Good"
            BaseType Var("Good Amulets") Var("Good Rings")
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Jewelry Color") 255

            Mixin "Item Level"
                Show "High"
                    ItemLevel >= Prop("High Item Level")
                    SetBorderColor Var("High Item Level Border Color")

    Mixin "Special"
        Show "Influence"
            HasInfluence "Shaper" "Elder" "Crusader" "Hunter" "Redeemer" "Warlord"
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Influence Color")
            MinimapIcon Medium Var("Influence Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Influence Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")
        Show "Fractured"
            FracturedItem True
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Synthesis Color") 200
            MinimapIcon Medium Var("Synthesis Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Synthesis Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")
        Show "Synthesised"
            SynthesisedItem True
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Synthesis Color")
            MinimapIcon Medium Var("Synthesis Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Synthesis Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")

    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

    Fork "Identified"
        Unset "False"
            Identified False
        Unset "True"
            Identified True

            Mixin "Mods"
                Show "Benche Craft"
                    HasExplicitMod Var("Bench Craft Mods")
                    SetBackgroundColor Var("Crafted Mod Color")
                    MinimapIcon Medium Var("Benche Craft Effect Color") Var("Accessory Minimap Icon Shape")
                    PlayEffect Var("Benche Craft Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Incursion"
                    HasExplicitMod Var("Incursion Mods")
                    SetBackgroundColor Var("Incursion Color")
                    MinimapIcon Medium Var("Incursion Effect Color") Var("Accessory Minimap Icon Shape")
                    PlayEffect Var("Incursion Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Delve"
                    HasExplicitMod Var("Delve Mods")
                    SetBackgroundColor Var("Delve Color")
                    MinimapIcon Medium Var("Delve Effect Color") Var("Accessory Minimap Icon Shape")
                    PlayEffect Var("Delve Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")
                Show "Betrayal"
                    HasExplicitMod Var("Betrayal Mods")
                    SetBackgroundColor Var("Betrayal Color")
                    MinimapIcon Medium Var("Betrayal Effect Color") Var("Accessory Minimap Icon Shape")
                    PlayEffect Var("Betrayal Effect Color")
                    PlayAlertSoundPositional Var("Lofty Sound")

Unset "Normal/Magic Jewelry"
    Class Var("Jewelries")
    SetFontSize Var("Small Font Size")
    SetBackgroundColor Var("Jewelry Color") 200

    Mixin "Base Type"
        Show "Atlas Base Accessories"
            BaseType Var("Atlas Base Accessories")
            SetFontSize Var("Extra Large Font Size")
            SetBackgroundColor Var("Atlas Color")
            MinimapIcon Medium Var("Atlas Effect Color") Var("Accessory Minimap Icon Shape")
            PlayEffect Var("Atlas Effect Color")
            PlayAlertSoundPositional Var("Lofty Sound")
        Unset "Good"
            BaseType Var("Good Amulets") Var("Good Rings")
            SetFontSize Var("Large Font Size")
            SetBackgroundColor Var("Jewelry Color") 255

            Mixin "Item Level"
                Show "High"
                    ItemLevel >= Prop("High Item Level")
                    SetBorderColor Var("High Item Level Border Color")
    Fork "Rarity"
        Prop("Magic Equipment Activity") "Magic"
            Rarity Magic
            SetTextColor Var("Magic Item Color")
        Unset "Normal"
            Rarity Normal
            SetTextColor Var("Normal Item Color")
    Fork "Corrupted"
        Unset "False"
            Corrupted False
        Unset "True"
            Corrupted True
            SetBorderColor Var("Corrupted Color")

