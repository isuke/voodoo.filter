    PlayAlertSoundPositional Var("Precious Sound")

    Mixin "Rareness"
        Show "Sublime"
            BaseType == Var("Sublime Corpses")
            SetFontSize Var("Extra Large Font Size")
            MinimapIcon Large Var("Affliction Effect Color") Var("League Minimap Icon Shape")
            PlayAlertSoundPositional Var("Sublime Sound")
        Show "Lofty"
            BaseType == Var("Lofty Corpses")
            SetFontSize Var("Large Font Size")
            MinimapIcon Large Var("Affliction Effect Color") Var("League Minimap Icon Shape")
            PlayAlertSoundPositional Var("Lofty Sound")

Show "An Audience With The King"
    BaseType "An Audience With The King"
    SetFontSize Var("Large Font Size")
    SetTextColor Var("Affliction Color")
    SetBackgroundColor Var("Precious BG Color")
    SetBorderColor Var("Affliction Color")
    MinimapIcon Large Var("Affliction Effect Color") Var("League Minimap Icon Shape")
    PlayEffect Var("Affliction Effect Color")
    PlayAlertSoundPositional Var("Lofty Sound")



