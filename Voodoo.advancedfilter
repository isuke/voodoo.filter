Show "Jeweller's Orb Recipe"
    Class Var("Equipments")
    Sockets = 6
    SetFontSize Var("Large Font Size")
    SetBackgroundColor Var("Jeweller's Orb Recipe Color")
    MinimapIcon Medium Var("Lofty Effect Color") Var("Equipment Minimap Icon Shape")
    PlayEffect Var("Lofty Effect Color")
    PlayAlertSoundPositional Var("Lofty Sound")

Prop("Chromatic Orb Recipe Activity") "Chromatic Orb Recipe"
    Class Var("Weapons") Var("Gears")
    SocketGroup RGB
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Chromatic Orb Recipe Color")
    SetBackgroundColor 0 0 0 200
    SetBorderColor Var("Chromatic Orb Recipe Color")
    MinimapIcon Small Var("Trivial Effect Color") Var("Equipment Minimap Icon Shape")
    PlayEffect Var("Trivial Effect Color") Temp


    Fork "Size"
        Unset "Small1"
            Height <= 3
            Width <= 1
        Unset "Small2"
            Height <= 2
            Width <= 2

Unset "Regal/Chaos Orb Recipe"
    Class Var("Weapons") Var("Gears")
    Rarity Rare
    Identified False
    SetFontSize Var("Small Font Size")

    Fork "Item Level"
        Unset "Regal"
            ItemLevel >= 75
            SetTextColor Var("Regal Orb Recipe Color")
            SetBorderColor Var("Regal Orb Recipe Color")
        Unset "Chaos"
            ItemLevel >= 60
            SetTextColor Var("Chaos Orb Recipe Color")
            SetBorderColor Var("Chaos Orb Recipe Color")

    Fork "Class"
        Unset "Weapon"
            Class Var("Weapons")
            SetBackgroundColor Var("Weapon Color") 160

            Fork "Size"
                Prop("Regal/Chaos Orb Recipe Activity") "Small"
                    Height <= 3
                    Width <= 1
        Prop("Regal/Chaos Orb Recipe Activity") "Gear"
            Class "Gloves" "Boots" "Body Armours" "Helmets"
            SetBackgroundColor Var("Gear Color") 160

Prop("Cartographer's Chisel Recipe Activity") "Cartographer's Chisel Recipe"
    BaseType "Stone Hammer" "Rock Breaker" "Gavel"
    Rarity Normal
    Corrupted False
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Hammer Color")
    SetBackgroundColor 0 0 0 100
    SetBorderColor Var("Hammer Color")

    Mixin "Quality"
        Unset "High"
            Quality > 10
            SetBackgroundColor Var("Tier2 BG Color") 240
        Unset "Middle"
            Quality > 0
            SetBackgroundColor Var("Tier3 BG Color") 200

Hide "Other Equipments"
    Class Var("Equipments")
    SetFontSize Var("Small Font Size")
    SetBackgroundColor 0 0 0 180

Hide "Currency"
    Class "Currency"
    BaseType Var("Currencies")
    SetTextColor Var("Currency Color")

    Fork "Rareness"
        Show "Sublime"
            BaseType Var("Sublime Currencies")
            SetFontSize Var("Extra Large Font Size")
            SetBackgroundColor Var("Sublime BG Color")
            MinimapIcon Largest Var("Sublime Effect Color") Var("Currency Minimap Icon Shape")
            PlayEffect Var("Sublime Effect Color")
            PlayAlertSoundPositional Var("Sublime Sound")

            Mixin "Base Type"
                Unset "Mirror of Kalandra"
                    BaseType "Mirror of Kalandra"
                    # PlayAlertSound ShMirror 300
                    CustomAlertSound "./Sounds/Mirror.wav"
                    DisableDropSound True
                Unset "Exalted Orb"
