                    Mixin "Item Level"
                        Show "High"
                            ItemLevel >= Prop("High Item Level")
                            SetBorderColor Var("High Item Level Border Color")
        Unset "Gear"
            Class Var("Gears")
            SetBackgroundColor Var("Gear Color") 200

            Mixin "Base Type"
                Show "Atziri Gears"
