            # PlayEffect Var("Atlas Effect Color") Temp
            # PlayAlertSoundPositional Var("Trivial Sound)

            Mixin "Item Level"
                Show "High"
                    ItemLevel >= 85
                    SetBorderColor Var("High Item Level Border Color")
                    MinimapIcon Small Var("Atlas Effect Color") Var("Equipment Minimap Icon Shape")
                    PlayEffect Var("Atlas Effect Color") Temp
                    PlayAlertSoundPositional Var("Trivial Sound")

        Hide "Good BaseType"
            BaseType "Warlock Boots" "Ambusher" "Ornate Quiver" "Grasping Mail"
            SetBackgroundColor Var("Experimented Color")

            Mixin "Item Level"
                Show "High"
                    ItemLevel >= 85
                    SetFontSize Var("Default Font Size")
                    SetBorderColor Var("High Item Level Border Color")

            Mixin "Special"
                Show "Influence"
