Unset "Cartographer's Chisel Recipe"
    BaseType "Stone Hammer" "Rock Breaker" "Gavel"
    Rarity Normal
    Corrupted False
    SetFontSize Var("Small Font Size")
    SetTextColor Var("Hammer Color")
    SetBackgroundColor 0 0 0 100
    SetBorderColor Var("Hammer Color")

    Mixin "Quality"
        Prop("Cartographer's Chisel Recipe Activity") "High"
            Quality > 10
            SetBackgroundColor Var("Tier2 BG Color") 240
        Prop("Cartographer's Chisel Recipe Activity") "Middle"
            Quality > 0
            SetBackgroundColor Var("Tier3 BG Color") 200

Unset "Regal/Choas Orb Recipe"
    Class Var("Weapons") Var("Gears")
    Rarity Rare
    Identified False
    SetFontSize Var("Small Font Size")

    Fork "Item Level"
        Unset "Regal"
            ItemLevel >= 75
            SetTextColor Var("Regal Orb Recipe Color")
            SetBorderColor Var("Regal Orb Recipe Color")
        Unset "Chaos"
            ItemLevel >= 60
            SetTextColor Var("Choas Orb Recipe Color")
            SetBorderColor Var("Choas Orb Recipe Color")

    Fork "Class 0"
        Unset "Weapon"
            Class Var("Weapons")
            SetBackgroundColor Var("Weapon Color") 200

            Fork "Size"
                Prop("Regal/Choas Orb Recipe Activity") "Small"
                    Height <= 3
                    Width <= 1
        Prop("Regal/Choas Orb Recipe Activity") "Gear"
            Class "Gloves" "Boots" "Body Armours" "Helmets"
            SetBackgroundColor Var("Gear Color") 200

            Mixin "Class 1"
                Unset "Glove and Boot"
                    Class "Gloves" "Boots"
                    SetBackgroundColor Lighten(50%)

